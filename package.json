{
  "name": "react-native-common",
  "version": "0.12.0",
  "description": "React Native common components",
  "main": "src/index.js",
  "scripts": {
    "start": "watchman watch-del-all && react-native start",
    "ios": "react-native run-ios --simulator=\"iPhone 8\"",
    "android": "react-native run-android",
    "pretest": "npm run lint",
    "lint": "eslint src -c .eslintrc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "android/",
    "ios/",
    "src/"
  ],
  "keyword": [
    "react",
    "react-native",
    "react-native-common",
    "ios",
    "android"
  ],
  "author": {
    "name": "Reza Ghorbani",
    "email": "r.ghorbani.f@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rghorbani/react-native-common/issues"
  },
  "dependencies": {
    "create-react-class": "~15.6.2",
    "fbjs": "~0.8.16",
    "immutable": "~3.8.2",
    "lodash": "^4.17.4",
    "prop-types": "~15.6.0",
    "react-native-animatable": "^1.2.4",
    "react-native-blur": "~3.2.2",
    "react-timer-mixin": "~0.13.3",
    "rebound": "~0.0.15"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-eslint": "~8.2.1",
    "babel-jest": "~22.1.0",
    "babel-preset-react": "~6.24.1",
    "babel-preset-react-native": "~4.0.0",
    "eslint": "~4.16.0",
    "eslint-plugin-react": "~7.5.1",
    "eslint-plugin-react-native": "~3.2.1",
    "jest": "~22.1.4",
    "prettier": "~1.5.3",
    "react": "16.0.0",
    "react-native": "0.51.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-vector-icons": "^4.5.0"
  },
  "prettier": {
    "requirePragma": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "parser": "flow"
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "src/components/navigator",
      "/node_modules/"
    ]
  },
  "rnpm": {
    "android": {
      "packageImportPath": "import com.kajoo.reactnativecommon.textinput.*;",
      "packageInstance": "new TextInputDelKeyHandlerPackage()"
    },
    "ios": {
      "project": "ios/RNCommon.xcodeproj"
    }
  },
  "homepage": "https://github.com/rghorbani/react-native-common",
  "contributors": [
    { "name": "rghorbani", "email": "r.ghorbani.f@gmail.com" }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rghorbani/react-native-common"
  }
}
